<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.AjouterPromotionController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: f2e6da;">
         <children>
            <TextField fx:id="nomPromo" layoutX="187.0" layoutY="47.0" />
