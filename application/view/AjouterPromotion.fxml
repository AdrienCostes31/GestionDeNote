            <TextField fx:id="nomFich" editable="false" layoutX="187.0" layoutY="95.0" />
            <Button fx:id="btn_valid" layoutX="275.0" layoutY="143.0" mnemonicParsing="false" onAction="#btnValider" style="-fx-background-color: d8b491; -fx-border-color: af855d; -fx-border-radius: 2;" text="Valider" />
            <Button fx:id="btn_parcour" layoutX="181.0" layoutY="143.0" mnemonicParsing="false" onAction="#parcourFichier" style="-fx-background-color: d8b491; -fx-border-color: af855d; -fx-border-radius: 2;" text="Parcourir..." />
            <Label layoutX="32.0" layoutY="51.0" text="Nom de la promotion :" />
